<?xml version="1.0" encoding="UTF-8"?>
<a:authorAction xmlns:a="http://www.oxygenxml.com/ns/author/external-action"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.oxygenxml.com/ns/author/external-action http://www.oxygenxml.com/ns/author/external-action/authorAction.xsd"
  id="table.split.above">
  <a:name>${i18n(table.split.above)}</a:name>
  <a:description>${i18n(table.split.above_description)}</a:description>
  <a:smallIconPath href="/images/TableSplitCellUp16.gif"/>
  <a:largeIconPath href="/images/TableSplitCellUp20.gif"/>
  <a:operations>
    <a:operation id="ro.sync.ecss.extensions.tei.table.SplitCellAboveBelowOperation">
      <a:xpathCondition>ancestor-or-self::*:cell[namespace-uri()='http://www.tei-c.org/ns/1.0']</a:xpathCondition>
      <a:arguments>
        <a:argument name="Split point">Above</a:argument>
      </a:arguments>
    </a:operation>
  </a:operations>
  <a:enabledInReadOnlyContext>false</a:enabledInReadOnlyContext>
</a:authorAction>
