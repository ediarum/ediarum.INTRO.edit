<?xml version="1.0" encoding="UTF-8"?>
<a:authorAction xmlns:a="http://www.oxygenxml.com/ns/author/external-action"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.oxygenxml.com/ns/author/external-action http://www.oxygenxml.com/ns/author/external-action/authorAction.xsd"
  id="p[renditon=#mMM]">
  <a:name>Absatz am Rand angestrichen</a:name>
  <a:operations>
    <a:operation id="ChangeAttributeOperation">
      <a:xpathCondition>ancestor-or-self::p[not(@rendition)]</a:xpathCondition>
      <a:arguments>
        <a:argument name="elementLocation">ancestor-or-self::p</a:argument>
        <a:argument name="name">rendition</a:argument>
        <a:argument name="value">#mMM</a:argument>
      </a:arguments>
    </a:operation>
    <a:operation id="ChangeAttributeOperation">
      <a:xpathCondition>ancestor-or-self::p[@rendition='#mMM']</a:xpathCondition>
      <a:arguments>
        <a:argument name="elementLocation">ancestor-or-self::p</a:argument>
        <a:argument name="name">rendition</a:argument>
        <a:argument name="value"/>
      </a:arguments>
    </a:operation>
  </a:operations>
  <a:enabledInReadOnlyContext>false</a:enabledInReadOnlyContext>
</a:authorAction>
